{
	"name": "next-prisma-auth-template",
	"version": "0.0.0",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/masonschafercodes/next-prisma-auth-template.git"
	},
	"scripts": {
		"dev": "concurrently -c green.bold,blue.bold 'yarn:dev:docker' 'yarn:dev:next'",
		"dev:docker": "docker compose up",
		"dev:next": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"typecheck": "tsc --noEmit",
		"prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
		"prettier:check": "prettier --check \"./src/**/*.{ts,tsx}\"",
		"db:generate": "prisma generate",
		"db:seed": "yarn prisma db seed",
		"db:migrate:dev": "yarn prisma migrate dev"
	},
	"dependencies": {
		"@prisma/client": "3.14.0",
		"clsx": "^1.1.1",
		"next": "12.1.6",
		"next-auth": "^4.3.4",
		"nprogress": "^0.2.0",
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"react-hook-form": "^7.31.2"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "^12.1.6",
		"@types/node": "17.0.35",
		"@types/nprogress": "^0.2.0",
		"@types/react": "18.0.9",
		"@types/react-dom": "18.0.4",
		"autoprefixer": "^10.4.7",
		"concurrently": "^7.2.1",
		"eslint": "8.16.0",
		"eslint-config-next": "12.1.6",
		"lint-staged": "^12.4.2",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prisma": "^3.14.0",
		"tailwindcss": "^3.0.24",
		"ts-node": "^10.8.0",
		"tsconfig-paths": "^4.0.0",
		"typescript": "4.6.4"
	},
	"browserslist": [
		"defaults",
		"not IE 11",
		"maintained node versions"
	],
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --cache --fix",
			"prettier --write"
		]
	},
	"prisma": {
		"seed": "ts-node --transpile-only -r tsconfig-paths/register ./prisma/seed.ts"
	}
}
